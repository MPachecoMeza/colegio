<h1>Estudiantes#new</h1>
<p>Find me in app/views/estudiantes/new.html.erb</p>

<%= simple_form_for @est do |f| %>
     <%= f.input :nombre %>
     <%= f.input :apellido %>
     <%= f.input :documento %>
     <%= f.input :fecha_nac %>
     <%= f.association :curso, label_method: :grado %>
     <%= f.submit "nuevo estudiante" %>
   <% end %>

<%= link_to "ver todos", estudiantes_path %>
