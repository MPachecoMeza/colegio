<h1>Profesores#index</h1>
<p>Find me in app/views/profesores/index.html.erb</p>

<table class="table table-striped table-hover" border=1 bordercolor="red"> <thead bgcolor="lightcyan">
  <tr><th scope="col"> ID </th> <td> Nombre </td> <th scope="row"> Apellido </th> <th>Documento</th>
  <th>Edad</th> </tr> </thead>

  <% @profs.each do |prof| %>
<tbody>
  <tr>
    <td> <%= prof.id %> </td>
    <td> <%= prof.nombre %></td>
    <td> <%= prof.apellido %></td>
    <td> <%= link_to prof.documento, profesor_path(prof) %> </td>
    <td> <%= prof.edad %></td>
  </tr>

<% end %>
</tbody>
</table>

<br>
